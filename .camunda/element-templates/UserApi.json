[
    {
        "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
        "name": "UserApi",
        "id": "connector.UserApi",
        "description": "",
        "version": 1,
        "appliesTo": [
          "bpmn:Task"
    ],
    "elementType": {
      "value": "bpmn:ServiceTask"
    },
    "properties": [
        {
        "label": "REST Method",
        "description": "Specify the HTTP method to use.",
        "type": "Dropdown",
        "choices": [
              { "name": "createUser", "value": "createUser" },
              { "name": "createUsersWithArrayInput", "value": "createUsersWithArrayInput" },
              { "name": "createUsersWithListInput", "value": "createUsersWithListInput" },
              { "name": "deleteUser", "value": "deleteUser" },
              { "name": "getUserByName", "value": "getUserByName" },
              { "name": "loginUser", "value": "loginUser" },
              { "name": "logoutUser", "value": "logoutUser" },
              { "name": "updateUser", "value": "updateUser" }
        ],
        "binding": {
          "type": "zeebe:taskHeader",
          "key": "method"
        }
      }
    ]
  }
]